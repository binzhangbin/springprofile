<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bin.zhang.vo.UserNS">
    <cache eviction="FIFO" readOnly="true" size="512" flushInterval="1000"/>
    <resultMap id="BlogMap" type="Blog">
        <id column="bid" property="bid"></id>
        <result column="btxt" property="btxt"></result>
        <result column="btitle" property="btitle"></result>
        <result column="uid" property="uid"></result>
    </resultMap>
    <!--查询单条:select与Vo字段名要Vo字段名不一致时-->
    <select id="findOneById" parameterType="java.lang.Integer" resultType="Blog">
        select bid,btxt,btitle,uid from blog where bid=#{bid}
    </select>
</mapper>